{
    "name": "project-root",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "clean": "pnpm -r run --parallel clean",
        "build": "pnpm -r run --aggregate-output build",
        "build-docs": "typedoc",
        "test": "pnpm -r run --aggregate-output test run",
        "check-types": "tsc",
        "watch-types": "pnpm check-types -w",
        "lint": "eslint .",
        "prettier-check": "pnpm prettier:common -c",
        "prettier": "pnpm prettier:common -w",
        "prettier:common": "prettier . --ignore-path .gitignore --ignore-path .prettierignore --cache"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^18.19.45",
        "concurrently": "^8.2.2",
        "esbuild": "^0.23.1",
        "eslint": "^9.9.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.27.0",
        "happy-dom": "^15.0.0",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "tsx": "^4.17.0",
        "typedoc": "^0.26.6",
        "typescript": "~5.5.4",
        "typescript-eslint": "^8.2.0",
        "vite": "^5.4.6",
        "vitest": "^2.0.5"
    },
    "engines": {
        "node": ">= 20",
        "pnpm": "^9"
    }
}
